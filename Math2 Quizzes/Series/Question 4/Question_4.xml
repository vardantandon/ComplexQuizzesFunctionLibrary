<quiz>  <!-- question: 1  -->
  <question type="algebra">
    <name>
      <text>Maclaurin Series</text>
    </name>
    <questiontext format="html">
      <text><![CDATA[<p>Use the Maclaurin series for tan(x) to find the first two-non zero terms of the Maclaurin expansion for tan<sup>2</sup>(x). </p>]]></text>
    </questiontext>
    <generalfeedback format="html">
      <text><![CDATA[<p>The Maclaurin expansion for tan(x) : <br>x + x^3/3 + (2*x^5)/15<br></p>]]></text>
    </generalfeedback>
    <defaultgrade>1.0000000</defaultgrade>
    <penalty>0.3333333</penalty>
    <hidden>0</hidden>
    <compareby>eval</compareby>
    <tolerance>0.001</tolerance>
    <nchecks>10</nchecks>
    <disallow><text></text></disallow>
    <allowedfuncs></allowedfuncs>
    <answerprefix><text>Answer(in x) = </text></answerprefix>
    <answer fraction="100" format="moodle_auto_format">
      <text>x^2 + (2*x^4)/3</text>
      <feedback format="html">
        <text></text>
      </feedback>
    </answer>
    <variable name="x">
        <min>1</min>
        <max>10</max>
    </variable>
